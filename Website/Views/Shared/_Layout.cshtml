                            @Html.Raw("<ul>")
                            for (int i = menuCount; i < listMenu.Count; i++)
                            {
                                Data.Model.Menu item = listMenu[i];
                                @Html.Raw("<li><a href='"+ item.Display +"' title='"+ item.Name +"'>"+ item.Name +"</a></li>")
                            }
                            @Html.Raw("</ul>")
                        }
                    </div>
                </div>
            </div>
            <div class="bottom padtop_3_0">
                <p>©2023 - @Helper.GlobalHelper.PageTitle | Bảo lưu tất cả các quyền</p>
                <ul>
                    @{
                        foreach (Data.Model.About item in listAbout)
                        {
                            string url = Helper.GlobalHelper.DomainURL + item.GetType().Name + "/" + item.Code + "-" + item.ID + ".html";
                            @Html.Raw("<li><a href='"+url +"' title='"+ item.Name +"'>"+ item.Name +"</a></li>")
                        }
                    }
                </ul>
            </div>
        </div>
    </footer>
    <div style="position:fixed; bottom:20px; right:20px; z-index:1000;">
        <div>
            <a href="https://zalo.me/@contact.Status" target="_blank">
                <img src="@Helper.GlobalHelper.DomainURLSub/image/zalo.png" width="50" height="50" />
