@inject Business.Interface.IAboutBusiness _aboutBusiness
@inject Business.Interface.IContactBusiness _contactBusiness
@inject Business.Interface.IIdeasBusiness _ideasBusiness
@inject Business.Interface.IMenuBusiness _menuBusiness
@inject Business.Interface.IQuoteBusiness _quoteBusiness
@inject Business.Interface.ISocialNetworkBusiness _socialNetworkBusiness

@{
    List<Data.Model.About> listAbout = await _aboutBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);

    List<Data.Model.Ideas> listIdeas = _ideasBusiness.GetByCondition(item => item.ParentID == Helper.GlobalHelper.CategoryLanguageID && item.IsHomePage == true && item.Active == true).ToList();

    Data.Model.Ideas ideas = new Ideas();
    if (listIdeas.Count > 0)
    {
        ideas = listIdeas[0];
        ideas.Note = Helper.GlobalHelper.DomainURL + ideas.GetType().Name + "/" + ideas.Code + "-" + ideas.ID + ".html";
    }

    List<Data.Model.Menu> listMenu = await _menuBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listMenu = listMenu.OrderBy(item => item.SortOrder).ToList();

    List<Data.Model.Quote> listQuote = await _quoteBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listQuote = listQuote.OrderBy(item => item.SortOrder).ToList();

    Data.Model.Quote quote01 = new Quote();
    if (listQuote.Count > 0)
    {
        quote01 = listQuote[0];
    }

    Data.Model.Quote quote02 = new Quote();
    if (listQuote.Count > 1)
    {
        quote02 = listQuote[1];
    }

    Data.Model.Quote quote03 = new Quote();
    if (listQuote.Count > 2)
    {
        quote03 = listQuote[2];
    }

    Data.Model.Quote quote04 = new Quote();
    if (listQuote.Count > 3)
    {
        quote04 = listQuote[3];
    }

    Data.Model.Quote quote05 = new Quote();
    if (listQuote.Count > 4)
    {
        quote05 = listQuote[4];
    }

    Data.Model.Quote quote06 = new Quote();
    if (listQuote.Count > 5)
    {
        quote06 = listQuote[5];
    }

    Data.Model.Quote quote07 = new Quote();
    if (listQuote.Count > 6)
    {
        quote07 = listQuote[6];
    }

    List<Data.Model.SocialNetwork> listSocialNetwork = await _socialNetworkBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listSocialNetwork = listSocialNetwork.OrderBy(item => item.SortOrder).ToList();

    Data.Model.SocialNetwork socialNetwork01 = new SocialNetwork();
    if (listSocialNetwork.Count > 0)
    {
        socialNetwork01 = listSocialNetwork[0];
    }

    Data.Model.SocialNetwork socialNetwork02 = new SocialNetwork();
    if (listSocialNetwork.Count > 1)
    {
        socialNetwork02 = listSocialNetwork[1];
    }

    Data.Model.SocialNetwork socialNetwork03 = new SocialNetwork();
    if (listSocialNetwork.Count > 2)
    {
        socialNetwork03 = listSocialNetwork[2];
    }

    Data.Model.Contact contact = await _contactBusiness.GetByIDAsync(1);
}
