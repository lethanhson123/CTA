        <div>
            <a href="@contact.Location" target="_blank">
                <img src="@Helper.GlobalHelper.DomainURLSub/image/messenger.png" width="50" height="50" />
            </a>
        </div>
        <div>
            <a href="tel:@contact.Status" target="_blank">
                <img src="@Helper.GlobalHelper.DomainURLSub/image/phone.png" width="50" height="50" />
            </a>
        </div>
    </div>
    <script type="text/javascript" src="@Helper.GlobalHelper.DomainURLSub/js/jquery.min.js"></script>
    <script type="text/javascript" src="@Helper.GlobalHelper.DomainURLSub/js/owl.carousel.js"></script>
    <script type="text/javascript" src="@Helper.GlobalHelper.DomainURLSub/js/wow.js"></script>    
    <script type="text/javascript" src="@Helper.GlobalHelper.DomainURLSub/js/news-dist.js"></script>
    <script type="text/javascript" src="@Helper.GlobalHelper.DomainURLSub/js/projects-dist.js"></script>
</body>
