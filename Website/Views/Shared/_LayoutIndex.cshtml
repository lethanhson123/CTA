@inject Business.Interface.IAboutBusiness _aboutBusiness
@inject Business.Interface.INewsBusiness _newsBusiness
@inject Business.Interface.ITeamBusiness _teamBusiness
@inject Business.Interface.IProjectBusiness _projectBusiness
@inject Business.Interface.IProjectFileBusiness _projectFileBusiness
@inject Business.Interface.IIdeasBusiness _ideasBusiness
@inject Business.Interface.IServiceBusiness _serviceBusiness
@inject Business.Interface.IBannerBusiness _bannerBusiness
@inject Business.Interface.IBannerFileBusiness _bannerFileBusiness

@{
    List<Data.Model.About> listAbout = await _aboutBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);

    List<Data.Model.Ideas> listIdeas = _ideasBusiness.GetByCondition(item => item.ParentID == Helper.GlobalHelper.CategoryLanguageID && item.IsHomePage == true && item.Active == true).ToList();

    Data.Model.Ideas ideas = new Ideas();
    if (listIdeas.Count > 0)
    {
        ideas = listIdeas[0];
        ideas.Note = Helper.GlobalHelper.DomainURL + ideas.GetType().Name + "/" + ideas.Code + "-" + ideas.ID + ".html";
    }

    List<Data.Model.News> listNews = await _newsBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listNews = listNews.OrderBy(item => item.SortOrder).ToList();
    Data.Model.News news = new News();
    if (listNews.Count > 0)
    {
        news = listNews[0];
        news.Note = Helper.GlobalHelper.DomainURL + news.GetType().Name + "/" + news.Code + "-" + news.ID + ".html";
    }

    List<Data.Model.Team> listTeam = await _teamBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listTeam = listTeam.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Team team = new Team();
    if (listTeam.Count > 0)
    {
        team = listTeam[0];
        team.Note = Helper.GlobalHelper.DomainURL + team.GetType().Name + "/" + team.Code + "-" + team.ID + ".html";
    }
    Data.Model.Team team001 = new Team();
    if (listTeam.Count > 1)
    {
        team001 = listTeam[1];
        team001.Note = Helper.GlobalHelper.DomainURL + team001.GetType().Name + "/" + team001.Code + "-" + team001.ID + ".html";
    }

    List<Data.Model.Project> listProject = _projectBusiness.GetByCondition(item => item.ParentID == Helper.GlobalHelper.CategoryLanguageID && item.IsHomePage == true && item.Active == true).ToList();
    listProject = listProject.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Project project001 = new Project();
    List<Data.Model.ProjectFile> listProjectFile = new List<ProjectFile>();
    if (listProject.Count > 0)
    {
        project001 = listProject[0];
        project001.Note = Helper.GlobalHelper.DomainURL + project001.GetType().Name + "/" + project001.Code + "-" + project001.ID + ".html";
        listProjectFile = await _projectFileBusiness.GetByParentIDAndActiveToListAsync(project001.ID, true);
    }

    List<Data.Model.Ideas> listIdeas001 = await _ideasBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listIdeas001 = listIdeas001.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Ideas ideas001 = new Ideas();
    if (listIdeas001.Count > 0)
    {
        ideas001 = listIdeas001[0];
        ideas001.Note = Helper.GlobalHelper.DomainURL + ideas001.GetType().Name + "/" + ideas001.Code + "-" + ideas001.ID + ".html";
    }

    List<Data.Model.Service> listService = await _serviceBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listService = listService.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Service service = new Service();
    if (listService.Count > 0)
    {
        service = listService[0];
        service.Note = Helper.GlobalHelper.DomainURL + service.GetType().Name + "/" + service.Code + "-" + service.ID + ".html";
    }

    Data.Model.Service service001 = new Service();
    if (listService.Count > 1)
    {
        service001 = listService[1];
        service001.Note = Helper.GlobalHelper.DomainURL + service001.GetType().Name + "/" + service001.Code + "-" + service001.ID + ".html";
    }

    Data.Model.Service service002 = new Service();
    if (listService.Count > 2)
    {
        service002 = listService[2];
        service002.Note = Helper.GlobalHelper.DomainURL + service002.GetType().Name + "/" + service002.Code + "-" + service002.ID + ".html";
    }

    Data.Model.Service service003 = new Service();
    if (listService.Count > 3)
    {
        service003 = listService[3];
        service003.Note = Helper.GlobalHelper.DomainURL + service003.GetType().Name + "/" + service003.Code + "-" + service003.ID + ".html";
    }

    List<Data.Model.Banner> listBanner = _bannerBusiness.GetByCondition(item => item.ParentID == Helper.GlobalHelper.CategoryLanguageID && item.IsHomePage == true && item.Active == true).ToList();
    listBanner = listBanner.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Banner banner = new Banner();
    List<Data.Model.BannerFile> listBannerFile = new List<BannerFile>();
    if (listBanner.Count > 0)
    {
        banner = listBanner[0];
        banner.Note = Helper.GlobalHelper.DomainURL + project001.GetType().Name + "/" + project001.Code + "-" + project001.ID + ".html";
        listBannerFile = await _bannerFileBusiness.GetByParentIDAndActiveToListAsync(banner.ID, true);
        listBannerFile = listBannerFile.OrderBy(item => item.SortOrder).ToList();
    }
}
