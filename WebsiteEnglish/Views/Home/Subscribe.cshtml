@inject Business.Interface.IContactBusiness _contactBusiness
@{
    Data.Model.Contact contact = await _contactBusiness.GetByIDAsync(1);
}
@{
    ViewData["Title"] = "Get the latest news from us";
    ViewData["Description"] = Helper.GlobalHelper.PageDescription;
    ViewData["URLCurrent"] = Helper.GlobalHelper.DomainURL + "subscribe.html";
    ViewData["URLImage"] = Helper.GlobalHelper.ImageURL;
}
<style>
    .btn-talk:hover {
        cursor: pointer;
        border-width: 0px;
    }
</style>
<script>
    function onSaveNewsletter() {
        $.ajax({
            type: "POST",
            datatype: "JSON",
            url: '@Url.Action("SaveNewsletter", "Home")',
            data: $('#formNewsletter').serialize(),
            success: function (result) {
                window.location.href = "/thank.html";
            },
            error: function (jqXHR, exception) { },
            complete: function () { },
        })
    }
</script>
