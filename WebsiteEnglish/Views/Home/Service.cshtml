@inject Business.Interface.IServiceBusiness _serviceBusiness
@inject Business.Interface.IProjectBusiness _projectBusiness
@inject Business.Interface.IMenuBusiness _menuBusiness
@{
    List<Data.Model.Service> listService = await _serviceBusiness.GetByParentIDAndActiveToListAsync(Helper.GlobalHelper.CategoryLanguageID, true);
    listService = listService.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Service service = new Service();
    if (listService.Count > 0)
    {
        service = listService[0];
        service.Note = Helper.GlobalHelper.DomainURL + service.GetType().Name + "/" + service.Code + "-" + service.ID + ".html";
    }

    Data.Model.Service service001 = new Service();
    if (listService.Count > 1)
    {
        service001 = listService[1];
        service001.Note = Helper.GlobalHelper.DomainURL + service001.GetType().Name + "/" + service001.Code + "-" + service001.ID + ".html";
    }

    Data.Model.Service service002 = new Service();
    if (listService.Count > 2)
    {
        service002 = listService[2];
        service002.Note = Helper.GlobalHelper.DomainURL + service002.GetType().Name + "/" + service002.Code + "-" + service002.ID + ".html";
    }

    Data.Model.Service service003 = new Service();
    if (listService.Count > 3)
    {
        service003 = listService[3];
        service003.Note = Helper.GlobalHelper.DomainURL + service003.GetType().Name + "/" + service003.Code + "-" + service003.ID + ".html";
    }

    List<Data.Model.Project> listProject = await _projectBusiness.GetByParentIDAndActiveToListAsync(1, true);
    listProject = listProject.OrderBy(item => item.SortOrder).ToList();
    Data.Model.Project project = new Project();
    if (listProject.Count > 0)
    {
        project = listProject[0];
        project.Note = Helper.GlobalHelper.DomainURL + project.GetType().Name + "/" + project.Code + "-" + project.ID + ".html";
    }

    Data.Model.Menu menu = await _menuBusiness.GetByIDAsync(Helper.GlobalHelper.MenuServiceID);
}

@{
    ViewData["Title"] = service.Name;
    ViewData["Description"] = service.Description;
    ViewData["URLImage"] = service.FileName;
    ViewData["URLCurrent"] = menu.Display;
}
